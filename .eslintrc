{
    "extends": "airbnb-base",
    "env": {
        "es2021": true,
        "node": true
    },
    "rules": {
        "brace-style": [
            "error",
            "stroustrup"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ],
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}